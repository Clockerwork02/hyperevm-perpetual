{
  "name": "hyperevm-perpetual",
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/arena_data.json",
      "dest": "/arena_data.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "index.html": {
      "includeFiles": "arena_data.json"
    }
  }
}